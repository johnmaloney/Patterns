<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net5.0</TargetFrameworks>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <RootNamespace>Patterns</RootNamespace>
    <AssemblyName>Patterns</AssemblyName>
    <RepositoryUrl>https://github.com/johnmaloney/Patterns.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <FileVersion Condition="'$(AssemblyFileVersion)'==''">1.0.0.0</FileVersion>
    <Version Condition="'$(NugetPackageVersion)'==''">1.0.0</Version>
    <WarningsAsErrors>NU1701,CS0162,CS0168,CS0219,CS0169,CS0105</WarningsAsErrors>
    <RunAnalyzersDuringBuild>true</RunAnalyzersDuringBuild>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
  </ItemGroup>
</Project>